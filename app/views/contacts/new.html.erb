



    <%= simple_form_for @contact do |f| %>
      <%= f.input :first_name %>
      <%= f.input :last_name %>
      <%= f.input :email %>
      <%= f.input :phone, label: "Phone (optional)" %>
      <%= f.input :subject, as: :check_boxes, collection: [['SAT'], ['ACT'],['Math'], ['Philosophy']], label: "Subject(s)" %>
      <%= f.input :grade, as: :select, collection: [['College student'], ['12th'], ['11th'], ['10th'], ['9th'], ['8th'], ['7th'], ['6th'], ['Other']], prompt: "Select", label: "Student grade" %>
      <%= f.input :message, label: "Message (optional)" %>
      <%= f.button :submit, "Submit" %>
    <% end %>
